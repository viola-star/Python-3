import random

def func(x, y) :
    print(random.randint(x,y))


print("Following program for rolling dice")
x="t";
while (x.lower()!="f"):
           func(1,6)
           print("Again?t or f")
           x = input()
           print(x)

#python considers any non empty string true so cant convert x to bool, have to compare strings
                           
