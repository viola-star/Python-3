#Guess the number!

import random

def diff(randno, userno):
    if(randno<userno):
        print("You have given too large a number")
    else:
        print("You have given too small a number")

def isno (x):
    if(type(x)==int):
        return "true"
    else:  
        return "false"


x= random.randint(0,10000)
y=int(input())
if (isno=="false"):
    print("Not actual no")
elif (x==y):
    print("Absolutely correct guess!")
else:
    diff(x,y)
        
